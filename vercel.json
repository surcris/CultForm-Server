{
    "version":2,
    "name":"cultform-server",
    "builds":[{ "src":"server.js","use":"@vercel/node"}],
    "routes":[
        { "src":"/(.*)", 
            "dest":"/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"] 
        },
        { "src":"/auth(.*)", 
            "dest":"/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"] 
        },
        { "src":"/perso(.*)", 
            "dest":"/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"] 
        },
        { "src":"/users(.*)", 
            "dest":"/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"] 
        }
    ],
    "rewrites": [
        { "source": "/perso/(.*)",
         "destination": "/perso" 
        },
        { "source": "/auth/(.*)",
         "destination": "/auth" 
        },
        { "source": "/users/(.*)",
         "destination": "/users" 
        }
    ]
    
}